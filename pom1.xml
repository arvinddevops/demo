<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <distributionManagement>
   <snapshotRepository>
      <id>RAJA</id>
      <url>http://localhost:8081/nexus/content/repositories/RAJA/</url>
   </snapshotRepository>
</distributionManagement>
	<modelVersion>4.0.0</modelVersion>
  <groupId>com.precise.in</groupId>
  <artifactId>samaple</artifactId>
  <packaging>war</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>samaple Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <finalName>samaple</finalName>
	  <plugins>
		  <plugin>
   <groupId>org.sonatype.plugins</groupId>
   <artifactId>nexus-staging-maven-plugin</artifactId>
   <version>1.6</version>
   <configuration>
      <serverId>RAJA</serverId>
      <nexusUrl>http://localhost:8081/nexus/content/repositories/RAJA/</nexusUrl>
      <skipStaging>true</skipStaging>
   </configuration>
</plugin>
		  </plugins>
  </build>
</project>
